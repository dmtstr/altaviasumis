{
  "definitions": {},
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "http://example.com/root.json",
  "type": "array",
  "required": [
    "HEADER",
    "BODY"
  ],
  "properties": {
    "BODY": {
      "$id": "#/properties/BODY",
      "type": "array",
      "required": [
        "HEAD",
        "BODY"
      ],
      "properties": {
        "HEAD": {
          "$id": "#/properties/BODY/properties/HEAD",
          "type": "array",
          "required": [
            "ORDER_NO",
            "CUSTOMER_REF",
            "ADDRESS"
          ],
          "properties": {
            "ORDER_NO": {
              "$id": "#/properties/BODY/properties/HEAD/properties/ORDER_NO",
              "type": "string",
              "pattern": "^(.*)$"
            },
            "CUSTOMER_REF": {
              "$id": "#/properties/BODY/properties/HEAD/properties/CUSTOMER_REF",
              "type": "string",
              "pattern": "^(.*)$"
            },
            "ADDRESS": {
              "$id": "#/properties/BODY/properties/HEAD/properties/ADDRESS",
              "type": "array",
              "required": [
                "ADDRESS_FIELD",
                "STREET_HOUSE_NO",
                "STREET",
                "HOUSE_NO",
                "COUNTRY_CODE",
                "ZIP_CODE",
                "CITY",
                "CONTACTS"
              ],
              "properties": {
                "ADDRESS_FIELD": {
                  "$id": "#/properties/BODY/properties/HEAD/properties/ADDRESS/properties/ADDRESS_FIELD",
                  "type": "string",
                  "pattern": "^(.*)$"
                },
                "STREET_HOUSE_NO": {
                  "$id": "#/properties/BODY/properties/HEAD/properties/ADDRESS/properties/STREET_HOUSE_NO",
                  "type": "string",
                  "pattern": "^(.*)$"
                },
                "STREET": {
                  "$id": "#/properties/BODY/properties/HEAD/properties/ADDRESS/properties/STREET",
                  "type": "string",
                  "pattern": "^(.*)$"
                },
                "HOUSE_NO": {
                  "$id": "#/properties/BODY/properties/HEAD/properties/ADDRESS/properties/HOUSE_NO",
                  "type": "string",
                  "pattern": "^(.*)$"
                },
                "COUNTRY_CODE": {
                  "$id": "#/properties/BODY/properties/HEAD/properties/ADDRESS/properties/COUNTRY_CODE",
                  "type": "string",
                  "pattern": "^(.*)$"
                },
                "ZIP_CODE": {
                  "$id": "#/properties/BODY/properties/HEAD/properties/ADDRESS/properties/ZIP_CODE",
                  "type": "string",
                  "pattern": "^(.*)$"
                },
                "CITY": {
                  "$id": "#/properties/BODY/properties/HEAD/properties/ADDRESS/properties/CITY",
                  "type": "string",
                  "pattern": "^(.*)$"
                },
                "CONTACTS": {
                  "$id": "#/properties/BODY/properties/HEAD/properties/ADDRESS/properties/CONTACTS",
                  "type": "array",
                  "required": [
                    "CONTACT"
                  ],
                  "properties": {
                    "CONTACT": {
                      "$id": "#/properties/BODY/properties/HEAD/properties/ADDRESS/properties/CONTACTS/properties/CONTACT",
                      "type": "array",
                      "items": {
                        "$id": "#/properties/BODY/properties/HEAD/properties/ADDRESS/properties/CONTACTS/properties/CONTACT/items",
                        "type": "array",
                        "required": [
                          "TYPE",
                          "NAME",
                          "VALUE"
                        ],
                        "properties": {
                          "TYPE": {
                            "$id": "#/properties/BODY/properties/HEAD/properties/ADDRESS/properties/CONTACTS/properties/CONTACT/items/properties/TYPE",
                            "type": "string",
                            "pattern": "^(.*)$"
                          },
                          "NAME": {
                            "$id": "#/properties/BODY/properties/HEAD/properties/ADDRESS/properties/CONTACTS/properties/CONTACT/items/properties/NAME",
                            "type": "string",
                            "pattern": "^(.*)$"
                          },
                          "VALUE": {
                            "$id": "#/properties/BODY/properties/HEAD/properties/ADDRESS/properties/CONTACTS/properties/CONTACT/items/properties/VALUE",
                            "type": "string",
                            "pattern": "^(.*)$"
                          }
                        }
                      }
                    }
                  }
                }
              }
            }
          }
        },
        "BODY": {
          "$id": "#/properties/BODY/properties/BODY",
          "type": "array",
          "required": [
            "LINE"
          ],
          "properties": {
            "LINE": {
              "$id": "#/properties/BODY/properties/BODY/properties/LINE",
              "type": "array",
              "required": [
                "ARTICLE",
                "ARTICLE_QUANTITY"
              ],
              "properties": {
                "ARTICLE": {
                  "$id": "#/properties/BODY/properties/BODY/properties/LINE/properties/ARTICLE",
                  "type": "array",
                  "required": [
                    "INT_NO"
                  ],
                  "properties": {
                    "INT_NO": {
                      "$id": "#/properties/BODY/properties/BODY/properties/LINE/properties/ARTICLE/properties/INT_NO",
                      "type": "string",
                      "pattern": "^(.*)$"
                    }
                  }
                },
                "ARTICLE_QUANTITY": {
                  "$id": "#/properties/BODY/properties/BODY/properties/LINE/properties/ARTICLE_QUANTITY",
                  "type": "array",
                  "required": [
                    "QUANTITY"
                  ],
                  "properties": {
                    "QUANTITY": {
                      "$id": "#/properties/BODY/properties/BODY/properties/LINE/properties/ARTICLE_QUANTITY/properties/QUANTITY",
                      "type": "string",
                      "pattern": "^(.*)$"
                    }
                  }
                }
              }
            }
          }
        }
      }
    }
  }
}