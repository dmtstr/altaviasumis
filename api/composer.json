{
    "name": "slim/slim-skeleton",
    "description": "A Slim Framework skeleton application for rapid development",
    "keywords": ["microframework", "rest", "router", "psr7"],
    "homepage": "http://github.com/slimphp/Slim-Skeleton",
    "license": "MIT",
    "authors": [
        {
            "name": "Josh Lockhart",
            "email": "info@joshlockhart.com",
            "homepage": "http://www.joshlockhart.com/"
        }
    ],
    "require": {
        "php": ">=7.1.0",
        "slim/slim": "^3.1",
        "slim/php-view": "^2.0",
        "monolog/monolog": "^1.17",
        "tuupola/slim-jwt-auth": "^3.1",
        "chadicus/slim-oauth2": "^3.1",
        "wsdl2phpgenerator/wsdl2phpgenerator": "^3.4",
        "vlucas/phpdotenv": "^2.5",
        "akrabat/rka-content-type-renderer": "^0.8.0",
        "tuupola/base62": "^0.11.1",
        "tuupola/slim-basic-auth": "^3.2",
        "tuupola/cors-middleware": "^0.9.1",
        "symfony/console": "^4.1",
        "justinrainbow/json-schema": "^5.2"
    },
    "require-dev": {
        "phpunit/phpunit": ">=4.8 < 6.0"
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/",
            "logic\\": "src/logic/",
            "App\\Command\\": "src/commands/"
        }
    },
    "config": {
        "process-timeout" : 0
    },
    "scripts": {
        "start": "php -S localhost:8080 -t public",
        "test": "phpunit"
    }

}
